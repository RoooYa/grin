page {
  background-color: #f5f5f7;
  color:#333;
  height: 100%;
}

.loading {
  text-align: center;
  font-size: 14px;
  color: #999;
  line-height: 60px;
}

.loading label {
  color: #ffb400;
}
.loading image {
  width: 32px;
  height: 32px;
}
.hover {
  color: #ffb400
}

.pay-icon {
  position: absolute;
  background: url(data:data:image/png;base64,) no-repeat center;
  background-size: cover;
  width: 50px;
  height: 50px;
  left: 50%;
  top: 50%;
  margin-left: -25px;
  margin-top: -25px;
}

@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

