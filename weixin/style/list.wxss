.v-list {
  background:#fff;
  margin-top: 10px;
}
.v-list .item {
  line-height:60px;
  border-bottom:solid #eee 1px;
  font-size: 18px;
  position: relative;
}
.v-list .item .img {
  position: absolute;
  width: 40px;
  height: 40px;
  top: 10px;
  right: 35px;
  border-radius: 4px;
}
.v-list .item label {
  color: #999;
  margin-right: 5px;
  font-size: 20px;
  margin-left: 10px;
}
.v-list .item icon {
  float: right;
  color: #bbb;
  margin-right: 10px;
}